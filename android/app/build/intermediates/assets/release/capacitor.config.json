{
	"appId": "com.let.app",
	"appName": "Let App",
	"webDir": "out",
	"server": {
		"hostname": "letpractice.firebaseapp.com",
		"androidScheme": "https"
	},
	"android": {
		"useLegacyBridge": false
	},
	"plugins": {
		"CapacitorCookies": {
			"enabled": true
		},
		"CapacitorHttp": {
			"enabled": false
		},
		"FirebaseAuthentication": {
			"skipNativeAuth": false,
			"providers": [
				"google.com"
			]
		},
		"DeepLinks": {
			"hosts": [
				"letpractice.firebaseapp.com"
			]
		}
	}
}
